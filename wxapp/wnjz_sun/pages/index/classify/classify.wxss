.searchIcon {
    width: 28rpx;
    height: 28rpx;
}

.search {
    background: #fff;
    height: 98rpx;
    display: flex;
    align-items: center;
    padding: 0rpx 20rpx;
    margin-bottom: 20rpx;
}

.search input {
    height: 60rpx;
    width: 556rpx;
    border: 2rpx solid #ececec;
    background: #f8f8f8;
    border-radius: 10rpx;
    font-size: 30rpx;
    padding-left: 20rpx;
    margin-right: 20rpx;
}

.search button {
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-right: 0rpx;
    padding-left: 0rpx;
    width: 110rpx;
    height: 64rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #41c2fc;
    border-radius: 10rpx;
}

.order-nav {
    width: 750rpx;
    height: 100rpx;
    background: #fff;
    line-height: 100rpx;
    white-space: nowrap;
    border-top: 1rpx solid #ececec;
}

.order-nav view {
    display: inline-block;
    width: 180rpx;
    height: 100rpx;
    text-align: center;
    font-size: 30rpx;
    color: #222;
}

.nav {
    display: inline-block;
    height: 100rpx;
}

.order-nav .on {
    box-sizing: border-box;
    color: #41c2fc;
    border-bottom: 2rpx solid #41c2fc;
}

.banner {
    width: 750rpx;
    height: 370rpx;
    margin-top: 20rpx;
    z-index: 999;
}

.banner>image {
    width: 750rpx;
    height: 370rpx;
}

.wrap {
    z-index: 1000;
    width: 750rpx;
    height: 100rpx;
    position: relative;
    top: -20rpx;
}

.wrap>view {
    width: 670rpx;
    margin: 0rpx auto;
    background: #fff;
    height: 120rpx;
    box-shadow: 0 0 12rpx 0 rgba(3,3,3,0.071);
    border-radius: 10rpx;
    margin-bottom: 20rpx;
    font-size: 34rpx;
    text-align: center;
    line-height: 120rpx;
}

.session {
    margin-top: 20rpx;
    padding: 0rpx 20rpx 0rpx;
    background: #fff;
}

.session-order-title {
    margin-left: 20rpx;
}

.session-wrap {
    display: flex;
    flex-direction: column;
    margin-top: 24rpx;
}

.session-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: 30rpx;
    padding-bottom: 26rpx;
    border-bottom: 1rpx solid #ececec;
}

.session-item:last-child {
    border-bottom: 0rpx solid #fff;
}

.item-left {
    display: flex;
    color: #999;
}

.order {
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
}

.item-title {
    font-size: 32rpx;
    line-height: 32rpx;
    margin-top: 6rpx;
    margin-bottom: 16rpx;
    color: #222;
}

.item-desc {
    width: 400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.item-price {
    margin-top: 18rpx;
    font-size: 26rpx;
    font-weight: 700;
    color: #41c2fc;
    display: flex;
    align-items: center;
}

.item-price>view {
    font-weight: 500;
    color: #222;
    font-size: 22rpx;
    margin-left: 30rpx;
}

.item-price>view>text {
    color: #41c2fc;
}

.item-price>view>text:first-child {
    margin-right: 20rpx;
}

.item-right {
    height: 64rpx;
    width: 124rpx;
    border: 2rpx solid #41c2fc;
    border-radius: 4rpx;
    font-size: 26rpx;
    font-weight: 700;
    color: #41c2fc;
    line-height: 64rpx;
    text-align: center;
}

.detCont {
    width: 414rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}