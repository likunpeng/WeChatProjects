<view class="container">
    <view>
        <view class="session">
            <view class="session-wrap">
                <view class="session-item" wx:for="{{cateData}}">
                    <navigator url="../serdesc/serdesc?id={{item.gid}}">
                        <view class="item-left" data-id="{{item.oid}}">
                            <image class="order" src="{{url}}{{item.pic}}"></image>
                            <view clss="detCont">
                                <view class="item-title">{{item.gname}}</view>
                                <view class="item-desc">{{item.probably}}</view>
                                <view class="item-price">￥{{item.shopprice}}</view>
                            </view>
                        </view>
                    </navigator>
                    <navigator url="../serdesc/serdesc?id={{item.gid}}">
                        <view class="item-right">预约</view>
                    </navigator>
                </view>
            </view>
        </view>
    </view>
</view>
