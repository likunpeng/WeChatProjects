.list {
    display: flex;
    width: 730rpx;
    padding: 20rpx 0rpx 20rpx 20rpx;
    height: 108rpx;
    background: #fff;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 10rpx;
}

.shopImg {
    width: 108rpx;
    height: 108rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}

.listLeft {
    display: flex;
}

.phoneIcon {
    width: 26rpx;
    height: 26rpx;
    margin-right: 20rpx;
}

.det {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.det>view {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #999;
    line-height: 28rpx;
}

.det>view>text {
    display: block;
    width: 392rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.det>view:first-child {
    font-size: 30rpx;
    color: #222;
    line-height: 32rpx;
}

.listRight {
    height: 108rpx;
    border-left: 1rpx dashed #dcdcdc;
    width: 120rpx;
    font-size: 26rpx;
    line-height: 36rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.listRight view:last-child {
    color: #999;
}

.det>view.curShop {
    font-size: 30rpx;
    color: #41c2fc;
    font-weight: 700;
    justify-content: flex-start;
}

.det>view.curShop>text {
    width: auto;
}