.wrap {
    position: relative;
    width: 750rpx;
}

.shopbg>image {
    width: 750rpx;
    height: 200rpx;
}

.list {
    display: flex;
    width: 680rpx;
    padding: 20rpx 0rpx 20rpx 30rpx;
    height: 108rpx;
    background: #fff;
    justify-content: space-between;
    position: absolute;
    left: 20rpx;
    bottom: 0rpx;
    z-index: 1111;
    border-radius: 12rpx 12rpx 0rpx 0rpx;
}

.shopImg {
    width: 108rpx;
    height: 108rpx;
    border-radius: 50%;
    margin-right: 24rpx;
}

.listLeft {
    display: flex;
}

.phoneIcon {
    width: 26rpx;
    height: 26rpx;
    margin-right: 20rpx;
}

.det {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.det>view {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #999;
}

.det>view>text {
    display: block;
    width: 352rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.det>view:first-child {
    font-size: 30rpx;
    color: #222;
}

.listRight {
    height: 58rpx;
    width: 120rpx;
    font-size: 26rpx;
    line-height: 36rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #41c2fc;
    color: #fff;
    border-radius: 29rpx 0rpx 0rpx 29rpx;
}

.listRight view:last-child {
    color: #999;
}

.changeIcon {
    width: 28rpx;
    height: 28rpx;
    margin-right: 10rpx;
}

.block {
    background: #41c2fc;
    color: #fff;
    height: 82rpx;
    display: flex;
    align-items: center;
    width: 710rpx;
    margin-left: 20rpx;
    box-shadow: 0 5rpx 8rpx 0rpx rgba(7,11,42,0.145);
    border-radius: 0rpx 0rpx 12rpx 12rpx;
}

.block>view {
    height: 70rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 365rpx;
    box-sizing: border-box;
}

.block>view:first-child {
    border-right: 1rpx solid #277497;
}

.block>view:last-child {
    border-left: 1rpx solid #dcdcdc;
}

.icn {
    width: 34rpx;
    height: 34rpx;
    margin-right: 20rpx;
}

.session {
    margin-top: 20rpx;
    background: #fff;
}

.sessionTitle {
    height: 92rpx;
    line-height: 92rpx;
    display: flex;
    align-items: center;
}

.sessionTitle::before {
    content: " ";
    display: inline-block;
    width: 10rpx;
    height: 30rpx;
    background: #41c2fc;
    border-radius: 5rpx;
    margin-right: 20rpx;
}

.navMin {
    width: 710rpx;
    padding-right: 20rpx;
    margin-left: 20rpx;
    display: flex;
    height: 92rpx;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #dcdcdc;
}

.unum {
    display: flex;
    align-items: center;
    color: #666;
    font-size: 24rpx;
}

.rightIcon {
    width: 16rpx;
    height: 24rpx;
    margin-left: 20rpx;
}

.opentime>text {
    margin-left: 20rpx;
    border-bottom: 0rpx solid #fff;
}

.cont {
    margin-top: 10rpx;
    background: #fff;
    padding-bottom: 30rpx;
}

.contTitle {
    height: 72rpx;
    color: #41c2fc;
    font-size: 30rpx;
    text-align: center;
    line-height: 72rpx;
}

.contain {
    line-height: 42rpx;
}

.contain image {
    width: 750rpx;
    margin-bottom: 20rpx;
}

.session-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-top: 30rpx;
    padding-bottom: 26rpx;
    border-bottom: 1rpx solid #ececec;
    padding: 40rpx 0rpx 24rpx;
    margin: 0rpx 20rpx;
    width: 710rpx;
}

.session-item:last-child {
    border-bottom: 0rpx solid #ececec;
}

.item-left {
    display: flex;
    color: #999;
}

.order {
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
}

.item-title {
    font-size: 32rpx;
    line-height: 32rpx;
    margin-top: 6rpx;
    margin-bottom: 16rpx;
    color: #222;
}

.item-desc {
    width: 400rpx;
    overflow: hidden;
    line-height: 32rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.item-price {
    margin-top: 18rpx;
    font-size: 26rpx;
    font-weight: 700;
    color: #41c2fc;
}

.item-right {
    height: 64rpx;
    width: 124rpx;
    border: 2rpx solid #41c2fc;
    border-radius: 4rpx;
    font-size: 26rpx;
    font-weight: 700;
    color: #41c2fc;
    line-height: 64rpx;
    text-align: center;
}

.detCont {
    width: 414rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}