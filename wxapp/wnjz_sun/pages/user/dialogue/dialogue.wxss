.session {
    padding: 0rpx 20rpx;
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #666;
}

.icon {
    width: 48rpx;
    height: 46rpx;
    margin-left: 42rpx;
}

.white {
    background: #fff;
    margin-bottom: 30rpx;
    font-size: 26rpx;
}

.session-title {
    color: #222;
    font-size: 30rpx;
}

.session view:last-child {
    display: flex;
    align-items: center;
}

.concat {
    background: #fff;
    font-size: 26rpx;
    display: flex;
    align-items: center;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-right: 0rpx;
    color: #666;
}

.concat:after {
    border: none;
}