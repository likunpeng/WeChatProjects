.user-info {
    height: 346rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(65,194,252) 0%,rgb(119,211,252)100%);
    display: flex;
    flex-direction: column;
    align-items: center;
    color: #fff;
}

.thumb {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    margin: 60rpx 0rpx 36rpx 0rpx;
    border: 2rpx solid #fff;
}

.user-count {
    display: flex;
    padding: 15rpx 0rpx;
    background: #fff;
}

.user-money {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 375rpx;
    height: 130rpx;
}

.user-money:first-child {
    width: 374rpx;
    border-right: 1px solid #ececec;
}

.icon {
    width: 42rpx;
    height: 28rpx;
    margin-right: 24rpx;
}

.m_up {
    margin-top: 26rpx;
}

.num {
    font-size: 42rpx;
    color: #666;
    font-weight: 700;
}

.m_dowm {
    margin-top: 26rpx;
    display: flex;
    align-items: center;
}

.session {
    padding: 0rpx 0rpx 0rpx 20rpx;
    background: #fff;
    margin-top: 20rpx;
}

.session-list {
    display: flex;
    height: 98rpx;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid #ececec;
    padding-right: 20rpx;
}

.session-lat {
    border-bottom: 0px solid #ececec;
}

.session-list view {
    display: flex;
    align-items: center;
    color: #666;
}

.iconlist {
    width: 32rpx;
    height: 34rpx;
    margin-right: 36rpx;
    margin-left: 10rpx;
}

.technical {
    width: 750rpx;
    display: flex;
    padding: 50rpx 0rpx;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    color: rgb(153,153,153);
}

.supportIcon {
    width: 64rpx;
    height: 64rpx;
    margin-right: 24rpx;
}

.supportPhone {
    width: 34rpx;
    height: 32rpx;
    margin-bottom: 8rpx;
}

.tecSup {
    line-height: 42rpx;
}

.dialogPhone {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-left: 60rpx;
    font-size: 20rpx;
}

#styleSecond .session1 {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 30rpx 26rpx;
    margin-top: 30rpx;
}

#styleSecond .session {
    width: 650rpx;
    margin-left: 20rpx;
    background: #fff;
    box-shadow: 0rpx 0rpx 14rpx 0rpx rgba(5,2,2,0.078);
    border-radius: 10rpx;
}

#styleSecond .session1 .lst {
    display: flex;
    align-items: center;
}

#styleSecond .session1 .lst>image {
    height: 88rpx;
    width: 88rpx;
    margin-right: 70rpx;
    border-radius: 50%;
    box-shadow: 1.5rpx 2.598rpx 8rpx 0rpx rgba(35,44,214,0.157);
    background-image: -webkit-linear-gradient(90deg,rgb(50,126,254) 0%,rgb(39,205,249) 100%);
    border: 2rpx solid #fff;
}

#styleSecond .session1 .toFx {
    width: 128rpx;
    height: 56rpx;
    line-height: 56rpx;
    border-radius: 28rpx;
    background-image: -webkit-linear-gradient(0deg,rgb(135,255,228) 0%,rgb(65,194,252) 100%);
    text-align: center;
    font-size: 26rpx;
    color: #fff;
    margin-right: 0px;
}