.session {
    background: #fff;
    padding: 0rpx 20rpx;
    line-height: 26rpx;
    margin-bottom: 20rpx;
}

.session:first-child {
    border-top: 1px solid #cbcbcb;
}

.session-up {
    height: 126rpx;
    display: flex;
    flex-direction: column;
}

.name {
    margin-top: 24rpx;
    font-size: 32rpx;
    line-height: 32rpx;
}

.addr {
    font-size: 26rpx;
    color: #999;
    margin-top: 22rpx;
}

.session-dowm {
    height: 82rpx;
    border-top: 1px solid #ececec;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.session-dowm view {
    display: flex;
    align-items: center;
    font-size: 26rpx;
    color: #666;
    margin-left: 30rpx;
}

.icon {
    width: 27rpx;
    height: 27rpx;
    margin-right: 16rpx;
}

.noaddr {
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 30rpx;
    color: #999;
}

.no {
    width: 368rpx;
    height: 226rpx;
    margin-top: 200rpx;
    margin-bottom: 20rpx;
}

.btn {
    width: 657rpx;
    height: 80rpx;
    font-size: 31rpx;
    border-radius: 10rpx;
    position: absolute;
    bottom: 20rpx;
}

.submit {
    color: #fff;
    background-color: #41c2fc;
    display: flex;
    align-items: center;
    justify-content: center;
}

.submit text {
    font-size: 50rpx;
    margin-right: 26rpx;
}