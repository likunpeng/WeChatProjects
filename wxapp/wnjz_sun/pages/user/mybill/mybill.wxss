page {
    background: #fff;
}

.container {
    line-height: 26rpx;
}

.bill {
    background: #F5F5F5;
    border-bottom: 1rpx solid #dcdcdc;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 30rpx;
    font-weight: 700;
}

.billDet {
    color: #666;
    font-size: 24rpx;
    margin-top: 18rpx;
    font-weight: 500;
}

.billDet>text {
    margin-right: 20rpx;
}

.dateIcon {
    width: 38rpx;
    height: 36rpx;
}

.session {
    padding-left: 30rpx;
    width: 720rpx;
}

.list {
    display: flex;
    justify-content: space-between;
    padding-right: 30rpx;
    height: 140rpx;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
    font-size: 32rpx;
}

.list:last-child {
    border-bottom: 0rpx solid #ececec;
}

.time {
    font-weight: 700;
    font-size: 26rpx;
    color: #999;
    margin-top: 18rpx;
}

.money {
    font-size: 30rpx;
    font-weight: 700;
}

.recharge {
    color: #41c2fc;
}