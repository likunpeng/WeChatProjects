.nav-group {
    display: flex;
    justify-content: space-around;
    align-items: center;
    background: #fff;
    color: #666;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
}

.nav {
    display: inline-block;
    height: 80rpx;
}

.on {
    color: #41c2fc;
    border-bottom: 4rpx solid #41c2fc;
    box-sizing: border-box;
}

.list {
    background: #fff;
    margin-top: 20rpx;
}

.list-up {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 0rpx;
    border-bottom: 1px solid #ececec;
    font-weight: 700;
}

.list-left {
    display: flex;
    padding-left: 20rpx;
}

.list-right {
    padding-right: 20rpx;
}

.left-det {
    width: 460rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 20rpx;
    color: #999;
    margin-top: 10rpx;
}

.left-title {
    color: #222;
    line-height: 42rpx;
    display: -webkit-box!important;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.img {
    height: 180rpx;
    width: 180rpx;
}

.list-down {
    height: 100rpx;
    background-color: #fff;
    display: flex;
    justify-content: flex-end;
    padding: 0rpx 20rpx;
    align-items: center;
}

.list-down .btn {
    height: 56rpx;
    font-size: 26rpx;
    color: #666;
    text-align: center;
    line-height: 56rpx;
    justify-content: center;
    border: 1rpx solid #666;
    margin-left: 14rpx;
    border-radius: 10rpx;
    background: #fff;
    margin-right: 0rpx;
    padding: 0rpx 10rpx;
}

.list-down .submit {
    color: #41c2fc;
    border: 1rpx solid #41c2fc;
}

.nobg {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    font-size: 34rpx;
    color: #999;
    font-weight: 700;
}

.nobg image {
    width: 170rpx;
    height: 170rpx;
    margin-top: 200rpx;
    margin-bottom: 50rpx;
}

.listHead {
    width: 710rpx;
    padding: 0rpx 20rpx;
    height: 88rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #ececec;
}