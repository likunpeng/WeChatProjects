.container {
    justify-content: flex-start;
    min-height: 100%;
    position: relative;
}

.user {
    height: 230rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(3,92,186)0%,rgb(65,194,252)100%);
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0rpx 30rpx 0rpx 22rpx;
    font-size: 28rpx;
    color: #fff;
}

.user-info {
    display: flex;
}

.nickname {
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-left: 30rpx;
}

.user-name {
    margin-bottom: 24rpx;
}

.thumb {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    border: 2rpx solid #fff;
}

.scan {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.icon {
    margin-bottom: 14rpx;
    width: 38rpx;
    height: 38rpx;
}

.session {
    background: #fff;
    font-size: 28rpx;
    color: #222;
    border-bottom: 1px solid #cbcbcb;
}

.session-cont {
    display: flex;
    flex-wrap: wrap;
}

.list {
    height: 120rpx;
    width: 335rpx;
    border-bottom: 1px solid #cbcbcb;
    padding: 20rpx;
    font-size: 28rpx;
    color: #222;
    display: flex;
    flex-direction: column;
}

.list:nth-child(odd) {
    width: 334rpx;
    border-right: 1px solid #cbcbcb;
}

.list-item {
    font-size: 52rpx;
    font-weight: 700;
    text-align: center;
    margin-top: 6rpx;
}

.order {
    margin-top: 20rpx;
    width: 750rpx;
    height: 162rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background: #fff;
    border-top: 1rpx solid #CBCBCB;
    border-bottom: 1rpx solid #CBCBCB;
}

.orderIcon {
    height: 82rpx;
    width: 82rpx;
    margin-bottom: 16rpx;
}

.orderList {
    width: 375rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    color: #222;
}

.off {
    background: #fff;
    margin-top: 20rpx;
    padding: 0rpx 30rpx 30rpx;
}

.offTitle {
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 30rpx;
}

.ordernum {
    border: 1rpx solid #ececec;
    width: 670rpx;
    border-radius: 8rpx;
    height: 72rpx;
    font-size: 28rpx;
    color: #222;
    padding-left: 20rpx;
}

.submit {
    height: 62rpx;
    line-height: 62rpx;
    width: 200rpx;
    font-size: 28rpx;
    color: #fff;
    margin-top: 20rpx;
    background: #41c2fc;
}

.outLogin {
    width: 750rpx;
    margin-top: 50rpx;
    display: flex;
    justify-content: center;
}

.outLogin button {
    width: 688rpx;
    height: 88rpx;
    line-height: 88rpx;
    color: #fff;
    font-size: 34rpx;
    background-image: -webkit-linear-gradient(90deg,rgb(34,184,255)0%,rgb(129,219,255)100%);
    border: 1rpx solid rgb(29,155,215);
}

.mask,.codemask {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    background: rgba(0,0,0,0.5);
    z-index: 1000;
}

.goos {
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translate(-50%);
    z-index: 9999;
    background: #fff;
    width: 650rpx;
    padding-bottom: 30rpx;
    border-radius: 10rpx;
}

.goodsdet {
    width: 400rpx;
    font-size: 26rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.btn {
    display: flex;
    justify-content: space-around;
    margin-top: 30rpx;
}

.btn>button {
    height: 60rpx;
    width: 160rpx;
    line-height: 60rpx;
    margin-left: 0rpx;
    margin-right: 0rpx;
    padding-left: 0rpx;
    padding-right: 0rpx;
    font-size: 28rpx;
    border: 1rpx solid #999;
    background: #fff;
}

.btn>button.confirm {
    background: #ef4c00;
    border: 1rpx solid #ef8200;
    color: #fff;
}

.list-left {
    margin-top: 10rpx;
    display: flex;
}

.img1 {
    height: 130rpx;
    width: 130rpx;
    margin-right: 20rpx;
}

.lists {
    padding: 20rpx;
}

.order-num {
    border-bottom: 1rpx solid #ececec;
    line-height: 40rpx;
    padding: 10rpx 0rpx;
    font-size: 30rpx;
}

.list-up {
    padding: 20rpx 0rpx 0rpx;
}

.left-title {
    line-height: 40rpx;
}

.code {
    position: absolute;
    top: 20%;
    left: 50%;
    transform: translate(-50%);
    z-index: 1001;
    background: #fff;
    width: 650rpx;
    height: 600rpx;
    padding-bottom: 30rpx;
    border-radius: 10rpx;
}

.qrcode {
    display: flex;
    align-items: center;
    margin: 30rpx auto;
    width: 420rpx;
}

.qrcode>canvas {
    width: 420rpx;
    height: 420rpx;
}

.codetitle {
    text-align: center;
    margin-top: 30rpx;
    font-size: 34rpx;
    color: #ef8200;
}

.showEwm {
    height: 96rpx;
    width: 96rpx;
    border-radius: 50%;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0px 0px 18px 0px rgba(4,0,0,0.055);
    margin-top: 30rpx;
    margin-left: 620rpx;
}

.showEwm>image {
    width: 56rpx;
    height: 56rpx;
}

.userOpe {
    background-color: #fff;
    padding: 34rpx 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    margin-top: 20rpx;
}

.opeartion {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.opeartion image {
    width: 58rpx;
    height: 58rpx;
}

.opeartion text {
    font-size: 26rpx;
    color: #666;
    margin-top: 20rpx;
}

.curPrice {
    font-size: 26rpx;
}